/* pages/login-rigister-chocie/login-rigister-chocie.wxss */.content {
  background: linear-gradient(181deg, rgba(13, 204, 255, 1) 0%, rgba(71, 96, 255, 1) 100%);
  display: flex;
  justify-content: center;
  align-items: center;
}

.login-container {
  width: 100%;
  display: flex;
  justify-content: space-around;
  flex-direction: column;
  align-items: center;
  height: 20%;
}

.personal-login {
  width: 80%;
  height: 90rpx;
  border-radius: 50rpx;
  padding: 2px;
  background-image: linear-gradient(0deg, rgba(13, 204, 255, 1) 0%, rgba(63, 112, 255, 1) 100%);
}

.enterprise-login {
  width: 80%;
  height: 90rpx;
  border-radius: 50rpx;
  padding: 2px;
  background-image: linear-gradient(0deg, rgba(13, 204, 255, 1) 0%, rgba(63, 112, 255, 1) 100%);
}

.box {
  width: 100%;
  height: 100%;
  border-radius: 50rpx;
  background: rgba(41, 150, 254);
}

.text {
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.center {
  display: flex;
  justify-content: center;
  align-items: center;
}

.back {
  width: 10px;
  height: 15px;
  position: fixed;
  top: 35px;
  left: 20px;
}
